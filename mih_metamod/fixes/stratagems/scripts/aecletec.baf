IF
  !Dead("tanar")  // ~Aec'Letec~
  Global("TanariXP","GLOBAL",1)
  HPLT(Myself,2)
  HitBy(LastHitter(Myself),ACID)
THEN
  RESPONSE #100
    SetInterrupt(FALSE)
    ChangeAnimationNoEffect("tanar2")
    ActionOverride(LastHitter("tanar"),ApplyDamagePercent("tanar2",100,ACID))
    SetGlobal("TanariXP","GLOBAL",2)
    SetInterrupt(TRUE)
END

