IF
  Name("tanar2",Myself)
  !Dead("tanar2")  // ~Aec'Letec~
  Global("TanariXP","GLOBAL",2)
THEN
  RESPONSE #100
    SetInterrupt(FALSE)
    Kill(Myself)
    AddexperienceParty(16000)
    SetGlobal("TanariXP","GLOBAL",3)
    SetInterrupt(TRUE)
END

IF
  !Dead("tanar")  // ~Aec'Letec~
  GlobalGT("TanariXP","GLOBAL",0)
  HPLT(Myself,2)
THEN
  RESPONSE #100
    ClearActions(Myself)
    MoveToObject(Myself)
    NoAction()
END
